Link - https://leetcode.com/problems/second-largest-digit-in-a-string/
Code - 
class Solution {
public:
    int secondHighest(string s) {
         set<int>v;
         for(int i=0;i<s.size();i++){
             if(s[i]>='0' && s[i]<='9'){
                 v.insert(s[i]-'0');
             }
         }
        if(v.size()<=1)
         return -1;
         else{ 
        auto it = v.begin();
         advance(it,v.size()-2);
         return *it;
         }
    }
};
